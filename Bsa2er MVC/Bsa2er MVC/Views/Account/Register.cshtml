                                                                                                                    {
                                                                                                                        <option selected="selected" value="male">معلم</option>
                                                                                                                        <option value="female">معلمة</option>
                                                                                                                    }
                                                                                                                    else
                                                                                                                    {
                                                                                                                        <option selected="selected" value="male">طالب </option>
                                                                                                                        <option value="female">طالبة</option>
