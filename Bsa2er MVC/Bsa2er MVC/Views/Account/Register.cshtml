                @Html.AntiForgeryToken()
                <div class="row">
                    <div class="col-xs-12 text-center">
                        @Html.ValidationSummary("", new { @class = "text-danger" })

                    </div>
                </div>

                <div class="form-row">
                    <div class="offset-5 col-3">
                        @Html.TextBoxFor(a => a.Email, new { @class = "form-control" })
                    </div>
                    <div class="col-2">
                        @Html.LabelFor(a => a.Email, "البريد الالكتروني", new { @class = "form-label" })
                    </div>
                </div>
                <div class="form-row mt-5">

                    <div class="offset-5 col-3">
                        @Html.TextBoxFor(a => a.Username, new { @class = "form-control" })
                    </div>
                    <div class="col-2">
                        @Html.LabelFor(a => a.Username, "اسم المستخدم", new { @class = "form-label" })
                    </div>
                </div>
                <div class="form-row mt-5">

                    <div class="offset-5 col-3">
                        @Html.TextBoxFor(a => a.fullname, new { @class = "form-control" })
                    </div>
                    <div class="col-2">
                        @Html.LabelFor(a => a.fullname, " الاسم كامل", new { @class = "form-label" })
                    </div>
                </div>
                <div class="form-row mt-5">

                    <div class="offset-5 col-3">
                        <select name="gender" style="direction:rtl;" class="form-control">
                            @if (ViewBag.id == "2")
                            {
                                <option selected="selected" value="male">مشرف</option>
                                <option value="female">مشرفة</option>
                            }
                            else if (ViewBag.id == "3")

                            {
                                <option selected="selected" value="male">معلم</option>
                                <option value="female">معلمة</option>
                            }
                            else if(ViewBag.id == "4")
                            {
                                <option selected="selected" value="male">طالب </option>
                                <option value="female">طالبة</option>
                            }

                        </select>

                    </div>
                    <div class="col-2">
                        @Html.LabelFor(a => a.gender, " نوع الطالب", new { @class = "form-label" })
                    </div>
                </div>

