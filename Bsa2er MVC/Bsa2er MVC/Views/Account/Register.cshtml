
@model Bsa2er_MVC.Models.RegisterViewModel
@{
    ViewBag.Title = "Register";
}
<div class="row">
    <div class="col-xs-12 mt-5 text-center">
        <div class="formwrapper">
            @using (Html.BeginForm("Register", "Account", new { id = ViewBag.id }, FormMethod.Post, new { @class = "form-horizontal", role = "form" }))
